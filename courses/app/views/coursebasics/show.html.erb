<%= form_for :coursebasics do |f| %>
    <div class="field">
      <%= f.label :title %><br>
      <%= f.text_area :title %>
    </div>
    <div class="field">
      <%= f.label :subtitle %><br>
      <%= f.text_area :subtitle %>
    </div>
    <div class="field">
      <%= f.label :language %><br>
      <%= f.number_field :language_id %>
    </div>
    <div class="field">
      <%= f.label :Level %><br>
      <%= f.number_field :level_id %>
    </div>
    <div class="field">
      <%= f.label :certificate %><br>
      <%= f.check_box :certificate %>
    </div>
    <div class="actions">
      <%= f.submit "Next"%>
    </div>
<% end %>
